<div class="span12">
  <div class="container cpu-progress-bars">
    {{#each cpus}}
      <div class="row">
        <div class="span1">
          <a href="/scheduling/cpu/{{num}}">CPU {{num}}</a>
        </div>
        <div class="span10">
          <div class="progress progress-info progress-striped active">
            <div class="bar" style="width: 0%"></div>
          </div>
        </div>
        <div class="span1 progress-display">
          0%
        </div>
      </div>
    {{/each}}
  </div>
</div>
